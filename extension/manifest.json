{
  "name": "Плагин по улучшению сайта sdo.tusur.ru",
  "description": "t.me/tarodictrl",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "*://sdo.tusur.ru/*",
        "*://ocenka.tusur.ru/*"
      ],
      "js": [ 
        "js/redirect.js",
        "js/filter.js",
        "js/TableCleaner.js",
        "js/pdf.js"
      ],
      "css": [
        "styles/sdo_theme.css"
      ],
      "run_at": "document_start"
    }
  ],
  "host_permissions": [
    "*://sdo.tusur.ru/*",
    "*://ocenka.tusur.ru/*"
  ]
}
